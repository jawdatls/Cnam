    		    android:layout_height="wrap_content"
    		    android:textAppearance="?android:attr/textAppearanceLarge"
    		    android:layout_weight=".33"
    		    android:textColor="#ff4a4a"
    		    android:textSize="16dp" />

    		<TextView
    		    android:id="@+id/week_income_td"
    		    android:layout_width="wrap_content"
    		    android:layout_height="wrap_content"
    		    android:textAppearance="?android:attr/textAppearanceLarge"
    		    android:layout_weight=".33"
    		    android:textColor="#529c52"
    		    android:textSize="16dp" />

    	</TableRow>
    	
    	<TableRow
    	    android:id="@+id/header_v_3"
    	    android:layout_width="fill_parent"
    	    android:layout_height="wrap_content"
    	    android:background="#3d3d3d" >

    		<TextView
    		    android:id="@+id/month_th"
    		    android:layout_width="wrap_content"
    		    android:layout_height="wrap_content"
    		    android:textAppearance="?android:attr/textAppearanceLarge"
    		    android:layout_weight=".33"
    		    android:textColor="#2f89df"
    		    android:textSize="16dp"  />

    		<TextView
    		    android:id="@+id/month_expense_td"
    		    android:layout_width="wrap_content"
    		    android:layout_height="wrap_content"
    		    android:textAppearance="?android:attr/textAppearanceLarge"
    		    android:layout_weight=".33"
    		    android:textColor="#ff4a4a"
    		    android:textSize="16dp" />

    		<TextView
    		    android:id="@+id/month_income_td"
    		    android:layout_width="wrap_content"
    		    android:layout_height="wrap_content"
